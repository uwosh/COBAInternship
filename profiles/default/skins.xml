<?xml version="1.0"?>

<object name="portal_skins"
        meta_type="Plone Skins Tool"
        allow_any="False"
        cookie_persistence="False"
        request_varname="plone_skin">

 <object name="cobainternship_images"
         meta_type="Filesystem Directory View"
         directory="COBAInternship/skins/cobainternship_images"/>
 <object name="cobainternship_styles"
         meta_type="Filesystem Directory View"
         directory="COBAInternship/skins/cobainternship_styles"/>
 <object name="cobainternship_templates"
         meta_type="Filesystem Directory View"
         directory="COBAInternship/skins/cobainternship_templates"/>
 
 <skin-path name="Plone Default">
  <layer insert-after="custom" 
         name="cobainternship_images"/>
  <layer insert-after="custom" 
         name="cobainternship_styles"/>
  <layer insert-after="custom" 
         name="cobainternship_templates"/>
 </skin-path>

</object>